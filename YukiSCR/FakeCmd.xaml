<Window x:Class="YukiSCR.FakeCmd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YukiSCR"
        mc:Ignorable="d"
        Title="Command Prompt" Height="300" Width="500">
    <Grid>
        <ScrollViewer Name="ScrollPane">
            <TextBox Name="Contents" Foreground="White" Background="Black" FontFamily="Consolas"></TextBox>
        </ScrollViewer>
    </Grid>
</Window>
